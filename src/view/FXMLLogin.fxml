<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="control.ControllerLogin">
   <children>
      <Label layoutX="100.0" layoutY="110.0" text="Login">
         <font>
            <Font name="Arial Bold" size="18.0" />
         </font>
      </Label>
      <Separator layoutX="100.0" layoutY="140.0" prefWidth="200.0" />
      <TextField fx:id="tfLogin" layoutX="100.0" layoutY="161.0" prefHeight="25.0" prefWidth="200.0" promptText="Login" />
      <Button fx:id="btnEntrar" layoutX="174.0" layoutY="271.0" mnemonicParsing="false" onAction="#AcaoBtnEntrar" style="-fx-background-color: #006400;" text="Entrar" textFill="WHITE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <PasswordField fx:id="pfSenha" layoutX="100.0" layoutY="200.0" prefHeight="25.0" prefWidth="200.0" promptText="Senha" />
   </children>
</AnchorPane>
